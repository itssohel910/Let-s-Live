{
  "manifest_version": 2,
  "name": "Suicide Site Alert",
  "version": "1.0",
  "description": "Alerts users about suicide-related sites.",
  "permissions": ["tabs", "http://*/*", "https://*/*"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    
  }
}
